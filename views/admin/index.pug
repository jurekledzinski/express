extends ../layout

block content
  h1= title
  nav
    ul
      li
        a(href='/admin/news/add') Dodaj
  table
    tr
      th id
      th tytuł
      th data dodania
      th akcja
    each item, index in data
      tr
        td=index
        td=item.title
        td=item.created
          a(href=`admin/news/delete/${item._id}`) usuń
          //- kilkamy w link tworzy sie adres endpoint z id z bazy danych pobrany 